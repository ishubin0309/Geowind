{% extends 'base.html.twig' %}

{% set title = 'Nouvelle liste' %}
{% set item = 'creer' %}

{% form_theme form 'bootstrap_3_horizontal_sm_layout.html.twig' %}

{% block body %}

  {{ form_start(form) }}
  <div class="container">

  <div class="row">
    <div class="col-xs-12">

      <fieldset>
        <legend>Nouvelle Liste</legend>
        <div class="col-xs-6">
        {{ form_row(form.listeFile) }}
        </div>
        <div class="col-xs-2">
          <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> Importer</button>
        </div>
      </fieldset>

    </div>
  </div>
  
  <div class="row">
    <div class="col-xs-12">
    
      {{ form_rest(form) }}
    </div>
  </div>


  </div>

      {{ form_end(form) }}

{% endblock %}