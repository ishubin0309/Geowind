        </div>
        <div style="margin-top: 10px;" class="col-md-12">
          <table>
            <tbody>
            <tr>
              <td colspan="8">
                <button type="button" id="projet_proprietaires_add_entry" class="btn btn-primary btn-xs"><i class="fa fa-plus-circle"></i> Ajouter</button>
{#                <button type="button" id="{{ id }}_duplicate_entry" class="btn btn-success disabled btn-xs"><i class="fa fa-plus-circle"></i> Dupliquer</button>#}
