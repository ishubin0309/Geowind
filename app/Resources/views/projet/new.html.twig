          <thead>
            <tr>
              <th>Fonction</th>
              <th>Utilisateur</th>
              <th>Téléphone</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>Source</td>
              <td class="required">{{ form_widget(form.origine, {'attr': {'style': 'width:100%'}}) }}</td>
              <td>{{ form_widget(form.origineTelephone) }}</td>
            </tr>
            {# {% if is_granted('ROLE_ADMIN') %} #}
            <tr>
              <td>Chef projet</td>
              <td class="required">{{ form_widget(form.chefProjet, {'attr': {'style': 'width:100%'}}) }}</td>
              <td>{{ form_widget(form.chefProjetTelephone) }}</td>
            </tr>
            {# {% endif %} #}
            <tr>
              <td>Chargé foncier</td>
              <td>{{ form_widget(form.chargeFoncier, {'attr': {'style': 'width:100%'}}) }}</td>
              <td>{{ form_widget(form.chargeFoncierTelephone) }}</td>
            </tr>
            <tr>
              <td>Partenaire</td>
              <td class="required">{{ form_widget(form.partenaire, {'attr': {'style': 'width:100%'}}) }}</td>
              <td>{{ form_widget(form.partenaireTelephone) }}</td>
            <tr>
            <tr>
              <td>Mairie</td>
              <td class="required">{{ form_widget(form.mairie, {'attr': {'style': 'width:100%'}}) }}</td>
              <td>{{ form_widget(form.mairieTelephone) }}</td>
            <tr>
            <tr>
