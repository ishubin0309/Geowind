            <tr>
            <tr id="contact_epci">
              <td>EPCI</td>
              <td class="required"></td>
              <td></td>
            </tr>
          </tbody>
        </table>
      </fieldset>

      <fieldset class="hide" id="tab_projet">
        <legend class="hide" id="form_site_terrain_gisement_legend">Gisement</legend>
